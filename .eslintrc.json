{
  "root": true,
  "parser": "@typescript-eslint/parser",
  "ignorePatterns": ["*.d.ts", "*.config.js", "dist", "node_modules", "out"],
   "parserOptions": {
    "project": ["tsconfig.json"]
},
  "env": {
    "browser": true
},
  "plugins": ["@typescript-eslint"],
"extends": [
    "eslint:all",
    "plugin:@typescript-eslint/stylistic-type-checked",
    "plugin:@typescript-eslint/strict-type-checked",
    "prettier"
  ],
  "rules": {
    "sort-imports": "off",
    "one-var": "off",
    "sort-keys": "off",
    "id-length": "off",
    "no-ternary": "off",
    "max-lines-per-function": "off",
    "max-statements": "off",
    "no-undefined": "off",
